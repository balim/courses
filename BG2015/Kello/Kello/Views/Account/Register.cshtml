@{
    ViewBag.Title = "Register";
}
@using (Html.BeginForm("Register"))
{
    @Html.Partial("_Credentials")
    <div>
        Repeat Password: @Html.Password("RepeatPassword")
    </div>
    <div>
        <input type="submit" value="Register" />
    </div>
}

@section Title
{
    <h2>Register!!!</h2>
}

@section scripts
{
    <script src="~/Scripts/jquery.validate.js"></script>
}
