@model IEnumerable<IssueTraq.Models.Issue>

@{
    ViewData["title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Hello World - Index</h2>

<ul>
    @foreach (var issue in Model)
